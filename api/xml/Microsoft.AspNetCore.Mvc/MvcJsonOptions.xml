      <MemberSignature Language="C#" Value="public bool AllowInputFormatterExceptionMessages { get; set; }" />
      <MemberSignature Language="ILAsm" Value=".property instance bool AllowInputFormatterExceptionMessages" />
      <MemberSignature Language="DocId" Value="P:Microsoft.AspNetCore.Mvc.MvcJsonOptions.AllowInputFormatterExceptionMessages" />

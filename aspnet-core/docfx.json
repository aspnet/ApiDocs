{
  "build": {
    "content": [
      {
        "files": [
          "**/*.md",
          "**/*.yml"
        ],
        "exclude": [
          "api/overwrites/**",
          "**/toc.yml",
          "**/obj/**",
          "**/includes/**",
          "README.md",
          "LICENSE",
          "LICENSE-CODE",
          "ThirdPartyNotices"
        ]
      },
      {
        "files": [
          "**/toc.yml"
        ],
        "src": ".",
        "dest": "aspnetcore_dotnet_toc"
      }
    ],
    "resource": [
      {
        "files": [
          "**/*.png",
          "**/*.jpg"
        ],
        "exclude": [
          "**/obj/**",
          "**/includes/**"
        ]
      }
    ],
    "overwrite": {
      "files": [
        "api/overwrites/**/*.md"
      ]
    },
    "xref": [
      "aspnetcore-legacy.yml"
    ],
    "externalReference": [],
    "globalMetadata": {
      "apiPlatform": "dotnet",
      "breadcrumb_path": "~/breadcrumb/toc.yml",
      "extendBreadcrumb": true,
      "ROBOTS": "INDEX,FOLLOW",
      "author": "dotnet-bot",
      "ms.author": "riande",
      "ms.date": "09/15/2016",
      "ms.topic": "managed-reference",
      "ms.reviewer": "daroth",
      "ms.prod": "aspnet-core-api",
      "searchScope": ["ASP.NET Core"] 
    },
    "fileMetadata": {},
    "template": [],
    "dest": "aspnetcore-dotnet-api",
    "lruSize": 0,
    "markdownEngineName": "markdig"
  }
}
